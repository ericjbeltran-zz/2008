import mx.transitions.Tween;import mx.transitions.easing.*;class ToggleButton extends MovieClip{	var btnToggle:Boolean;	function ToggleButton(){		this.btnToggle = true;		this.onPress = togglePress;	}	function togglePress(){		if(this.btnToggle){			var twScaleUpX:Tween = new Tween(this, "_xscale", Elastic.easeOut,100,150,1,true);			var twScaleUpY:Tween = new Tween(this, "_yscale", Elastic.easeOut,100,150,1,true);			this.btnToggle = false;		}else{			var twScaleDownX:Tween = new Tween(this, "_xscale", Elastic.easeOut,150,100,1,true);			var twScaleDownY:Tween = new Tween(this, "_yscale", Elastic.easeOut,150,100,1,true);			this.btnToggle = true;		}	}}