class AdvButton extends MovieClip {	var mcLabel:MovieClip;	var txtLabel:TextField;	//var btnText:String;	public function AdvButton() {		//this.btnText = "home button";		//trace(this.btnText);		//trace(this._name);		this.mcLabel.txtLabel.text = this._name;		this.onRollOver = this.over;		this.onRollOut = this.out;	}	function over() {		this.gotoAndPlay("over");	}	function out() {		this.gotoAndPlay("out");	}}